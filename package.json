{
  "name": "abui",
  "version": "0.0.0",
  "description": "A sleek, customizable React component library that lets you build stunning, responsive, and accessible web apps in no time!",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest  --coverage --verbose",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html}\"",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dubey748/abui.git"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "author": "ABhishek Dubey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dubey748/abui/issues"
  },
  "homepage": "https://github.com/dubey748/abui#readme",
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "8.3.3",
    "@storybook/addon-essentials": "8.5.3",
    "@storybook/addon-interactions": "8.5.3",
    "@storybook/addon-onboarding": "8.5.3",
    "@storybook/blocks": "8.5.3",
    "@storybook/react": "8.5.3",
    "@storybook/react-vite": "8.5.3",
    "@storybook/test": "8.5.3",
    "@types/react": "^19.0.8",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-storybook": "^0.11.2",
    "exact": "^1.0.1",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "8.5.3",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "dependencies": {
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.1",
    "rollup": "^4.34.0",
    "rollup-plugin-dts": "^6.1.1",
    "styled-components": "^6.1.15",
    "tslib": "^2.8.1"
  }
}
